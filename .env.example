# Jira Cloud
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your.email@company.com
JIRA_API_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxx

# Domyślny JQL do sync (np. bieżący sprint lub okres)
JQL=sprint = "BE 4.2 2025 (21.10-03.11)" OR updated >= -14d

# Cron: co 30 min (UTC serwera)
CRON_SCHEDULE=*/30 * * * *

# Port serwera
PORT=8080
